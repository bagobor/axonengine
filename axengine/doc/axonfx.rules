<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Axon FX files"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="build-axon-fx"
			DisplayName="Axon Effect files"
			CommandLine="rem cgc.exe -DNOT_IN_ENGINE=1 -DG_OPENGL=1 -po EXT_gpu_shader4,version=150 [VPENTRY] [VPPROFILE] &quot;$(InputPath)&quot;&#x0D;&#x0A;rem cgc.exe -DNOT_IN_ENGINE=1 -DG_OPENGL=1 -po EXT_gpu_shader4,version=150 [FPENTRY] [FPPROFILE] &quot;$(InputPath)&quot;&#x0D;&#x0A;fxc.exe /Gec /DNOT_IN_ENGINE=1 /DG_D3D=1 /I$(InputDir) /T fx_2_0 &quot;$(InputPath)&quot;&#x0D;&#x0A;fxc.exe /Gec /DNOT_IN_ENGINE=1 /DG_D3D=1 /DG_DX11=1 /I$(InputDir) /T fx_5_0 &quot;$(InputPath)&quot;"
			Outputs="$(InputName).vp"
			AdditionalDependencies="*.fxh"
			FileExtensions="*.fx"
			ExecutionDescription="Compiling Axon Effect file &quot;$(InputPath)&quot;"
			BatchingSeparator=" ;"
			>
			<Properties>
				<StringProperty
					Name="VPENTRY"
					DisplayName="Entry name"
					Category="OpenGL Vertex Program"
					Description="Entry name(-entry name)"
					Switch="-entry [value]"
					DefaultValue="VP_main"
				/>
				<StringProperty
					Name="VPPROFILE"
					DisplayName="Profile"
					Category="OpenGL Vertex Program"
					Description="Profile(-profile name )"
					Switch="-profile [value]"
					DefaultValue="glslv"
				/>
				<StringProperty
					Name="FPENTRY"
					DisplayName="Entry"
					Category="OpenGL Fragment Program"
					Description="Entry(-entry name)"
					Switch="-entry [value]"
					DefaultValue="FP_main"
				/>
				<StringProperty
					Name="FPPROFILE"
					DisplayName="Profile"
					Category="OpenGL Fragment Program"
					Description="Profile(-profile name)"
					Switch="-profile [value]"
					DefaultValue="glslf"
				/>
				<StringProperty
					Name="VSENTRY"
					DisplayName="Vertex Shader Entry"
					Category="D3D Vertex Shader"
					Switch="/E [value]"
					DefaultValue="VP_main"
				/>
				<StringProperty
					Name="VSPROFILE"
					DisplayName="Vertex Shader Profile"
					Category="D3D Vertex Shader"
					Switch="/T [value]"
					DefaultValue="vs_3_0"
				/>
				<StringProperty
					Name="PSENTRY"
					DisplayName="Pixel Shader Entry"
					Category="D3D Pixel Shader"
					Switch="/E [value]"
					DefaultValue="FP_main"
				/>
				<StringProperty
					Name="PSPROFILE"
					DisplayName="Pixel Shader Profile"
					Category="D3D Pixel Shader"
					Switch="/T [value]"
					DefaultValue="ps_3_0"
				/>
			</Properties>
		</CustomBuildRule>
		<CustomBuildRule
			Name="luac"
			DisplayName="luac"
			CommandLine="luac.exe -l -o &quot;$(InputDir)generated\$(InputName).luaobj&quot; $(InputPath)"
			Outputs="$(InputDir)generated\$(InputName).luaobj"
			FileExtensions="*.lua"
			ExecutionDescription="Lua compiling..."
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
